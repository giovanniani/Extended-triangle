let
	var num := 3;
	var car := 4
in

	put('O');
	put(' ');
	putint(num);
	puteol();
	put('A');
	put(' ');
	num := 2;
	putint(num);
	puteol();
	puteol();

	repeat while (num < 10) do
		putint(num);
		put(' ');
		num := num +1
	end;

	puteol();
	puteol();
	car := car * num;
	putint(car);
	puteol()

end