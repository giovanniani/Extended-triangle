! Calculates the fibonnaci sequence.
! Luis Leopoldo PÃ©rez, Sep. 2005
let
	var i: Integer;
	var j: Integer;
	var k: Integer;
	var num: Integer
in

	getint (var num); ! Number of iterations.
	i:= 1;
	j:= 2;
	repeat while (num > 0) do
		putint(i);
		put(' ');
		k:= i+j;
		i:= j;
		j:= k;
		num:= num - 1; skip
	end;

	skip


end