let
  var i : Integer;
  var j : Integer
in
begin

  i := 1;
  j := 1;

  while i < 4 do
  begin

    j := 1;
    while j < 10 do
    begin

      putint(i);
      put(' ');
      putint(j);
      puteol();
      j := j + 1

    end;

    puteol();
    i := i + 1

  end

end
